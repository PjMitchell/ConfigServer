{
    "description": "Provides configurators a way of configuring associated client configuration through a simple web interface with meta data and validation.",
    "version": "1.0.0-beta1-*",
    "packOptions": {
        "repository": {
            "type": "git",
            "url": "git://github.com/PjMitchell/ConfigServer"
        }
    },
    "buildOptions": { "xmlDoc": true },
    "dependencies": {
        "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0",
        "Microsoft.AspNetCore.Http.Abstractions": "1.0.0",
        "ConfigServer.Core": "1.0.0-*",
        "NETStandard.Library": "1.6.0"
    },

    "frameworks": {
        "netstandard1.6": {
            "imports": [
                "dotnet5.6",
                "dnxcore50"
            ]
        },
        "netcoreapp1.0": {
            "imports": "dnxcore50"
        }
    }
}
