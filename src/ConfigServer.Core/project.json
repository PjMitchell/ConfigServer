{
    "description": "Handling of application configuration storage and management across multiple instances.",
    "authors": [ "Peter Mitchell" ],
    "version": "1.0.0-beta1-*",
    "packOptions": {
        "repository": {
            "type": "git",
            "url": "git://github.com/PjMitchell/ConfigServer"
        }
    },
    "buildOptions": { "xmlDoc": true },
    "dependencies": {
        "Microsoft.AspNetCore.Http.Abstractions": "1.0.0",
        "Microsoft.Extensions.DependencyInjection.Abstractions": "1.0.0",
        "Newtonsoft.Json": "9.0.1",
