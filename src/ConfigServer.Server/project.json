{
    "description": "Provides configurators a way of configuring associated client configuration through a simple web interface with meta data and validation.",
    "authors": [ "Peter Mitchell" ],
    "version": "1.0.0-beta3-*",
    "packOptions": {
        "repository": {
            "type": "git",
            "url": "git://github.com/PjMitchell/ConfigServer"
        },
        "licenseUrl": "https://github.com/PjMitchell/ConfigServer/blob/master/LICENSE.txt"
    },
    "buildOptions": { "xmlDoc": true },

    "dependencies": {
        "ConfigServer.Core": "1.0.0-*",
        "Microsoft.AspNetCore.Http.Abstractions": "1.0.0",
        "NETStandard.Library": "1.6.0",
        "Newtonsoft.Json": "9.0.1"
    },

    "frameworks": {
        "net451": {
            "frameworkAssemblies": {
                "System.ComponentModel.DataAnnotations": "4.0.0.0"
            }
        },
        "net452": {
            "frameworkAssemblies": {
                "System.ComponentModel.DataAnnotations": "4.0.0.0"
            }
        },
        "netstandard1.6": {
            "imports": [
                "dotnet5.6",
                "dnxcore50"
            ],
            "dependencies": {
                "System.Reflection.TypeExtensions": "4.1.0",
                "System.Text.RegularExpressions": "4.1.0",
                "System.ComponentModel.Annotations": "4.1.0"
            }
        },
        "netcoreapp1.0": {
            "imports": "dnxcore50",
            "dependencies": {
                "System.Reflection.TypeExtensions": "4.1.0",
                "System.Text.RegularExpressions": "4.1.0",
                "System.ComponentModel.Annotations": "4.1.0"
            }
        }
    }
}
